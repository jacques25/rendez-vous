{% extends 'base.html.twig' %}

{% block title %} {{page.title}} {% endblock %}

{% block body %}
<div class="row content-page">
{% if page.filename is defined %}
     <img class="jumbotron img-fluid"  src="{{ vich_uploader_asset(page, 'imageFile')}}" >
 {% endif %} 
<div class="justify-content-center">
 <h1>{{page.title}}</h1>

 <div class="paragraph mb-5">{{page.description|raw}}</div>
 </div>
 </div>
{% endblock %}
