{{ form_start(form) }}
<div class="row flex-row">
  <div class="col">
    {{ form_row(form.title) }}
  </div>
  <div class="col">
    {{ form_row(form.category,{'id' : 'multiple'}) }}
  </div>
</div>

  
   


<div class="row">
  <div class="col-6">
    {{ form_row(form.description)}}
  </div> 
  <div class="col-6">
  {{ form_row(form.photo)}}
  </div>
</div>
<div class="row">
  <button class="btn btn-success">
    {{ button_label|default('Save') }}</button>
</div>
{{ form_end(form) }}

