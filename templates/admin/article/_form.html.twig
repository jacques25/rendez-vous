{{ form_start(form) }}
<div class="row">
  <div class="col-md-4">
    {{ form_row(form.title) }}</div>
  <div class="col-md-4">
    {{ form_row(form.category) }}</div>
</div>
<div class="row">
  <div class="col-md-8">
    {{ form_row(form.description)}}
  </div>
</div>
<div class="row">
 
  <div class="col-md-8">
    {{ form_row(form.imageFile)}}
  </div>
</div>

{{ form_rest(form) }}
<button class="btn btn-success mb-0">
  {{ button_label|default('Save') }}</button>
{{ form_end(form) }}
